<%- include('../includes/head.ejs') %>
  </head>

  <body>
    <%- include('../includes/navigation.ejs') %>

    <main>
      <h1>Your Orders</h1>
      <% if (orders.length > 0) { %>
        <ul>
          <% for (let order of orders) { %>
            <li>
              <h1># <%= order.id %></h1>
              <ul>
                <% for (let product of order.products) { %>
                  <li><%= product.title %> (<%= product.orderItem.quantity %>)</li>
                <% } %>
              </ul>
            </li>
          <% } %>
        </ul>
      <% } else { %>
        <h1>No Order</h1>
      <% } %>
    </main>

  </body>
  <%- include('../includes/end.ejs') %>